<Application
    x:Class="Minesweeper.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:Minesweeper"
    StartupUri="View/MainWindow.xaml">
    <Application.Resources>

    
        <!--Color brushes for Stroke, Main, Highlight, Secondary, Tertiary-->

        <SolidColorBrush x:Key="Stroke" Color="#272343"></SolidColorBrush>
        <SolidColorBrush x:Key="Main" Color="#fffffe"></SolidColorBrush>
        <SolidColorBrush x:Key="Highlight" Color="#ffd804"></SolidColorBrush>
        <SolidColorBrush x:Key="Secondary" Color="#e3f6f5"></SolidColorBrush>
        <SolidColorBrush x:Key="Tertiary" Color="#bae8e8"></SolidColorBrush>
        <ImageBrush x:Key="Background" ImageSource="Assets/tiledbg.jpg" TileMode="Tile" Viewport="0,0,200,200" ViewportUnits="Absolute"></ImageBrush>
        <FontFamily x:Key="Roboto">/Minesweeper;component/Assets/Roboto-Black.ttf#Roboto Black</FontFamily>



    </Application.Resources>


</Application>
